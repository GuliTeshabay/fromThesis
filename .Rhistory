# Form x(t) - x(t-p) with missing values for initial conditions
dif<-function (x,p){
nr<-dim(x)[1]
dx<-matrix(NA, nrow=dim(x)[1], ncol=dim(x)[2])
dx[(p+1):nr,]=x[(p+1):nr,]-x[1:(nr-p),]
return(dx)
}
u<- matrix(c(1, 22, 45, 96 ,74, 85, 14, 25, 96, 1 ,22, 45, 96 ,74, 85, 14, 25, 96), 18, 1)
guli<-dif(u,2)
guli<-dif(u,1)
lag_func <- function (x, p) {
nr <- dim(x)[1]
xlag<-matrix(data=NA, dim(x)[1], dim(x)[2])
if (p>=0){
xlag[(p+1):nr,] = x[1:(nr-p),]
} else {
xlag[1:(nr+p),] = x[(1-p):nr,]
}
return(xlag)
}
u<- matrix(c(1, 22, 45, 96 ,74, 85, 14, 25, 96, 1 ,22, 45, 96 ,74, 85, 14, 25, 96), 18, 1)
guli<-lag_func(u, 2)
